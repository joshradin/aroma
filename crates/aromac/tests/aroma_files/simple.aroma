namespace simple.system
import aroma.system.Object
import aroma.system.Closure


public class Box[T] delegate T {
    public static delegate BoxEater(Box[T]) -> T;

    private data: T;
    private ref_count: Long;
    private func: BoxEater;

    public constructor(data: T) {
    }

    public fn getData() -> T {
        return data;
    }

    public fn setData(data: T) {
        this.data = data;
    }

    public static fn box[T](data: T) -> Box[T] {
        return Box(data);
    }
}



